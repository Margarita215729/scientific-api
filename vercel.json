{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ui/$1"
    },
    {
      "src": "/ads",
      "dest": "/ui/ads.html"
    },
    {
      "src": "/",
      "dest": "/ui/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "ENVIRONMENT": "production"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}