{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ui/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/ui/favicon.ico"
    },
    {
      "src": "/(api|docs|openapi.json|redoc).*",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*\\.(js|css|html|ico))",
      "dest": "/ui/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}