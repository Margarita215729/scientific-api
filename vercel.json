{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ui/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/ui/favicon.ico"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "api/vercel_app.py"
    }
  ]
}